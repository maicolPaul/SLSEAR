
@{
    ViewBag.Title = "Index";
}


<input type="hidden" id="hdnusuario" value="@ViewBag.usuario" />

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header cardheadertitle">
                <h4 class="card-title">Sear Evaluados</h4>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            <label class="form-label">Departamento</label>
                            <select class="form-control form-select select2" data-bs-placeholder="Seleccione" id="cboDepartamento">
                                <option label="Seleccione">Seleccione</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="form-group">
                            <h4>Listad de Sear Evaluados</h4>
                            <table class="table table-bordered text-nowrap border-bottom dataTable" id="tblsears">
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalelegirganador" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Seleccionar Ganador</h5>
                <button class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <span>Esta Seguro de Elegir Ganador al Sear Seleccionado?</span>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-secondary" data-bs-dismiss="modal" id="btncerrar">Cerrar</button>
                <button class="btn btn-primary" id="btnelegirganador" type="button">Ok</button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/Evaluados/Index.js?noCache=@Guid.NewGuid().ToString()"></script>
